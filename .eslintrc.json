{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [
      "warn",
      "never"
    ],
    "jsx-a11y/img-has-alt": 0,
    "react/prop-types": 0,
    "global-require": 0,
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": [
      0,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
